#!/bin/bash
#SBATCH --job-name="ISATNN"
#SBATCH --output="ISATNN.%j.%N.out"
#SBATCH --partition=day-long-cpu
#SBATCH --nodes=1
#SBATCH --nodelist=node43
#SBATCH --ntasks-per-node=30
#SBATCH --export=ALL
#SBATCH -t 04:15:00

./b1 1500 1 > outPost15 &
./b1 1600 1 > outPost16 &
./b1 1700 1 > outPost17 &
./b1 1800 1 > outPost18 &
./b1 1900 1 > outPost19 &
./b1 2000 1 > outPost20 &
./b1 2100 1 > outPost21 &
./b1 2200 1 > outPost22 &
./b1 2300 1 > outPost23 &
./b1 2400 1 > outPost24 &
./b1 2500 1 > outPost25 &
./b1 2600 1 > outPost26 &
./b1 2700 1 > outPost27 &
./b1 2800 1 > outPost28 &
./b1 2900 1 > outPost29 &
./b1 3000 1 > outPost30 &

wait

cat *1.dat > output_217.dat

./b1 217 0 > outPost217

mv results.dat results1.dat

wait

./b2 1500 1 > outPost15 &
./b2 1600 1 > outPost16 &
